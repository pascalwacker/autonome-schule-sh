{% extends 'AppBundle::base.html.twig' %}
{% block body %}
    {% if home %}
        {% set curHome = home|first() %}
        {% if slides %}
            <header class="topslider">
            <style type="text/css">
                @media screen and (min-width: 769px) {
                    .topslider {
                        height: 100%;
                        min-height: 100%;
                        overflow: hidden;
                        margin-bottom: 11px;
                    }
                    
                    .topslider > .flexslider, .topslider > .flexslider > .slides, .topslider > .flexslider > .slides > li {
                        height: 100%;
                        margin-bottom: 0;
                    }
                    
                    .topslider > .flexslider > .slides > li {
                        background-size: cover;
                        background-repeat: no-repeat;
                        background-position: center top;
                    }
                    
                    .topslider > .flexslider > .slides > li > img {
                        display: none;
                    }
                }
            </style>
            <!-- flexslider -->
            <div class="flexslider wrapper100percent">
            <ul class="slides">
                {% for slide in slides %}
                    {% if slide.image %}
                        <li style="background-image: url('{{ vich_uploader_asset(slide, 'imageFile') }}" alt="" /></li>
                    {% endif %}
                {% endfor %}
            </ul>
            </div>
            <!-- flexslider end -->
            {% if curHome.sliderIcon or curHome.sliderTitle or curHome.sliderSubTitle %}
                <section class="slidertexthide slidertext startAnimation animated" data-animate="fadeInUp">
                    <div class="slidertextinner">
                        {% if curHome.sliderIcon %}<img src="{{ curHome.sliderIcon }}" />{% endif %}
                        {% if curHome.sliderTitle %}<h1>{{ curHome.sliderTitle }}</h1>{% endif %}
                        {% if curHome.sliderSubTitle %}<h2>{{ curHome.sliderSubTitle }}</h2>{% endif %}
                    </div>
                </section>
            {% endif %}
            </header> 
            <!-- topslider end -->
        {% endif %}
        {% if curHome.title or curHome.lead %}
            <div class="undersliderwrapper">  
                <div class="container"> 
                    <div class="row"> 
                        <section class="col-lg-12 underslider startAnimation animated" data-animate="fadeInUp">  
                            {% if curHome.title %}<h3>{{ curHome.title }}</h3>{% endif %}
                            {% if curHome.lead %}<h4>{{ curHome.lead|nl2br }}</h4>{% endif %}
                        </section> 
                    </div> 
                </div>
            </div> 
        {% endif %}
    {% endif %}
    {% if goal %}
        <!-- one section --> 
        <section class="wrapper100percent section1 serviceswrapper">
        <div class="sectionwrapper" id="1"></div> 
        <div class="container">
        <div class="row">
            <div class="col-lg-12">
            <div class="mainheadlinewrapper">
                <div class="mainheadline startAnimation animated" data-animate="fadeInUp">
                <h2>Unsere Ziele</h2>
                </div>
            </div>
            </div>
        </div>
        <!-- icons columns --> 
        <div class="services">
        <div class="row servicecolumn">
            {% for entry in goal %}
                <!-- one column --> 
                <div class="col-sm-4 servicecolumnone startAnimation animated" data-animate="slideInLeft">
                {% if entry.icon %}<img src="{{ entry.icon }}" alt=""/>{% endif %}
                {% if entry.title %}<h4><span>{{ entry.title }}</span></h4>{% endif %}
                {% if entry.text %}<p>
                    {{ entry.text|nl2br }}
                </p>{% endif %}
                </div>
                <!-- one column end--> 
            {% endfor %}
        </div>
        </div>
        <!-- icon columns end -->
        </div>
        <!-- container end --> 
        </section>
        <!-- one section end -->
    {% endif %}
    {% if events %}
        <!-- one section --> 
        <section class="wrapper100percent section2">
        <div id="2" class="sectionwrapper"></div> 
        <div class="container">
        <div class="row">
            <div class="col-lg-12">
            <div class="mainheadlinewrapper">
                <div class="mainheadline  textleft startAnimation animated" data-animate="fadeInUp">
                <h2>Nächste Veranstaltungen</h2>
                </div>
            </div>
            </div>
        </div> 
        <div class="row startAnimation animated" data-animate="fadeInUp">
            <!-- events --> 
            <div class="col-sm-8 eventswrapper">
            {% for event in events %}
                <!-- one event --> 
                <div class="oneevent">
                    <div class="eventsdate">
                    <p><span>{{ event.date|date('d.') }}</span></p>
                    <p>{{ event.date|date('M') }}</p>
                    <p>{{ event.date|date('Y') }}</p>
                    </div>
                    <div class="eventstext">
                    <h4>{{ event.name }}</h4>
                    <ul>
                    <li>Ort: <b>{{ event.place }}</b></li>
                    <li>Zeit: <b>{{ event.date|date('H:i') }}</b></li>
                    </ul>
                    {% if event.lead %}<p>{{ event.lead }}</p>{% endif %}
                    <a class="eventslink" href="#">mehr Informationen <i class="fa fa-chevron-right"></i></a>
                    </div>
                </div>
                <!-- one event end --> 
            {% endfor %}
            </div>
            <!-- events end --> 
            <div class="col-sm-4">
            <h5><span>Komm vorbei und engagiere dich!</span></h5>
            <p>Leitfaden/Grundsätze der ASS. Wir setzen uns für eine Barierenfreie Bildung für alle ein, unabhängig von ..., ..., ... und so weiter.
            </p>
            <p>Leitfaden/Grundsätze der ASS. Wir setzen uns für eine Barierenfreie Bildung für alle ein, unabhängig von ..., ..., ... und so weiter.
            </p>
            <p>Leitfaden/Grundsätze der ASS. Wir setzen uns für eine Barierenfreie Bildung für alle ein, unabhängig von ..., ..., ... und so weiter.
            </p>
            <p>Leitfaden/Grundsätze der ASS. Neben klassischen Bildungsangeboten veranstalten wir auch ..., ..., ...
            </p>  
            <p>Haben wir dein Interesse geweckt, dann komm vorbei!
            </p>
            <a class="btn btn-5 btn-5a icon-chevron-right" href="#"><span>Weitere Veranstaltungen</span></a>
            </div>
            <!-- col-sm-4 end --> 
        </div>
        <!-- row end --> 
        </div>
        <!-- container end --> 
        </section>
        <!-- one section end --> 
    {% endif %}
